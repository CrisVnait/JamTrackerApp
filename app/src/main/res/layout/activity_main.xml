<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="tracker.jam.app.dhbw.com.jamtrackerapp.MainActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="240dp"
        android:layout_height="160dp"
        android:layout_marginBottom="8dp"
        android:onClick="testButton"
        android:text="@string/button_test"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textViewNord"
            android:layout_width="225dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:text="@string/cam_ka_nord"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewNordDensity"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginEnd="24dp"
            android:text="@string/placeholder"
            android:textAlignment="textEnd"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textViewNord"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewNord" />

        <TextView
            android:id="@+id/textViewMitte"
            android:layout_width="225dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:text="@string/cam_ka_mitte"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewNord" />

        <TextView
            android:id="@+id/textViewMitteDensity"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginEnd="24dp"
            android:text="@string/placeholder"
            android:textAlignment="textEnd"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textViewMitte"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewMitte" />

        <TextView
            android:id="@+id/textViewDreieck"
            android:layout_width="225dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:text="@string/cam_ka_dreieck"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewMitte" />

        <TextView
            android:id="@+id/textViewDreieckDensity"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginEnd="24dp"
            android:text="@string/placeholder"
            android:textAlignment="textEnd"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textViewDreieck"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewDreieck" />

        <TextView
            android:id="@+id/textViewEttlingen"
            android:layout_width="225dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:text="@string/cam_ettlingen"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewDreieck" />

        <TextView
            android:id="@+id/textViewEttlingenDensity"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginEnd="24dp"
            android:text="@string/placeholder"
            android:textAlignment="textEnd"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textViewEttlingen"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewEttlingen" />

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
